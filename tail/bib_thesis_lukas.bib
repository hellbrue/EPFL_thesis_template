	author = {Couch, David E. and Hickstein, Daniel D. and Winters, David G. and Backus, Sterling J. and Kirchner, Matthew S. and Domingue, Scott R. and Ramirez, Jessica J. and Durfee, Charles G. and Murnane, Margaret M. and Kapteyn, Henry C.},
	urldate = {2023-04-28},
	date = {2020-07-20},
	note = {Publisher: Optica Publishing Group},
	keywords = {Free electron lasers, Harmonic generation, Hollow core fibers, Mass spectrometry, Phase matching, Photonic crystal fibers},
	file = {Full Text PDF:C\:\\Users\\hellbrue\\Zotero\\storage\\BZGYX85K\\Couch et al. - 2020 - Ultrafast 1 MHz vacuum-ultraviolet source via high.pdf:application/pdf},
}

