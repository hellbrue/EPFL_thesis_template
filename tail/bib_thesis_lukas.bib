	pages = {013155},
	number = {1},
	journaltitle = {Physical Review Research},
	shortjournal = {Phys. Rev. Res.},
	author = {Mitsuishi, Natsuki and Sugita, Yusuke and Akiba, Tomoki and Takahashi, Yuki and Sakano, Masato and Horiba, Koji and Kumigashira, Hiroshi and Takahashi, Hidefumi and Ishiwata, Shintaro and Motome, Yukitoshi and Ishizaka, Kyoko},
